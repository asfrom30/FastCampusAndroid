<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.doyoon.android.fastcampusandroid.week3.webview.WebViewMain">

    <WebView
        android:id="@+id/webview"
        android:layout_width="368dp"
        android:layout_height="0dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/runtimePermission_et_inputUrl" />

    <Button
        android:id="@+id/runtimePermission_btn_back"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/btn_prev"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        app:layout_constraintRight_toLeftOf="@+id/runtimePermission_et_inputUrl"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/runtimePermission_btn_foward"
        android:layout_width="0dp"
        android:layout_height="41dp"
        android:text="@string/btn_next"
        app:layout_constraintBottom_toBottomOf="@+id/runtimePermission_btn_go"
        app:layout_constraintTop_toTopOf="@+id/runtimePermission_btn_go"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/runtimePermission_btn_go" />

    <Button
        android:id="@+id/runtimePermission_btn_go"
        android:layout_width="0dp"
        android:layout_height="36dp"
        android:text="Go"
        app:layout_constraintBottom_toBottomOf="@+id/runtimePermission_et_inputUrl"
        app:layout_constraintTop_toTopOf="@+id/runtimePermission_et_inputUrl"
        app:layout_constraintRight_toLeftOf="@+id/runtimePermission_btn_foward"
        app:layout_constraintLeft_toRightOf="@+id/runtimePermission_et_inputUrl"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/runtimePermission_et_inputUrl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="102dp"
        app:layout_constraintBottom_toBottomOf="@+id/runtimePermission_btn_back"
        app:layout_constraintTop_toTopOf="@+id/runtimePermission_btn_back"
        app:layout_constraintVertical_bias="0.6" />
</android.support.constraint.ConstraintLayout>
